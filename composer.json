{
    "name": "amranidev/lpackager",
    "description": "Packages Generator for Laravel",
    "homepage": "http://github.com/amranidev/scaffold-interface",
    "keywords": ["laravel", "framework", "package generator", "package"],
    "license": "MIT",
    "authors": [{
        "name": "Amrani Houssain",
        "email": "amranidev@gmail.com"
    }],
    "require": {
        "php": ">=5.5.0",
        "illuminate/support": "~5.1"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.0"
    },
    "autoload": {
        "psr-4": {
            "Amranidev\\Lpackager\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Amranidev\\Lpackager\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "testbench": "composer require --dev \"orchestra/testbench=~3.0\""
    },
}